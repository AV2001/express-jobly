{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/jobUpdate.json",
    "type": "object",
    "default": {},
    "title": "Job Update Schema",
    "properties": {
        "title": {
            "type": "string",
            "title": "The title Schema",
            "examples": ["Senior Software Engineer"]
        },
        "salary": {
            "type": "integer",
            "title": "The salary Schema",
            "examples": [250000]
        },
        "equity": {
            "type": "number",
            "minimum": 0,
            "maximum": 1,
            "title": "The equity Schema",
            "examples": [0.05]
        }
    },
    "additionalProperties": false,
    "examples": [
        {
            "title": "Senior Software Engineer",
            "salary": 250000,
            "equity": 0.05
        }
    ]
}
